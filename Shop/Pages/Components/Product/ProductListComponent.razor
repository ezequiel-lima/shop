<ul>
    @foreach (var item in _products)
    {
        <li>
            @item.Name
        </li>
    }
</ul>

@code {
    private List<Product> _products = new();

    protected override async Task OnInitializedAsync()
    {
        for (int i = 0; i < 25; i++)
        {
            _products.Add(new Product(i + 1, $"Produto {i}", $"Descrição do produto {i}", "$imagem-prod-{i}", i * 13.75M, 1));
        }
    }
}
